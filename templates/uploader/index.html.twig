{% extends 'base.html.twig' %}
{% dump %}
{% block title %}Hello UploaderController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <ul id="title" name="title">

        {% for elem in dogs %}
            <li><a href="{{ path( 'uploader_carrousel', {id: elem.carrousel.id}) }}"> {{ elem.name }}</a></li>
        {% endfor %}
    </ul>
</div>

{% endblock %}
